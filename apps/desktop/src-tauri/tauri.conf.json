{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Crownix Vault",
  "version": "1.1.0",
  "identifier": "dev.crownix.vault",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Crownix Vault",
        "width": 1050,
        "height": 750,
        "resizable": false,
        "fullscreen": false,
        "devtools": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "endpoints": ["https://raw.githubusercontent.com/tsgamage/crownix-vault-releases/main/latest.json"],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEU0QkI1MzlDOEU5MzM4RUQKUldUdE9KT09uRk83NUlJVHhYK0EyOGlUcUY0OFllOEJxRkkxYUt5djNKaHdnOENoVE52aTNoSy8K"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
    "createUpdaterArtifacts": true
  }
}
